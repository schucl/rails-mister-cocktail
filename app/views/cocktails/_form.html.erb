<div class="container">
  <%= simple_form_for(@cocktail) do |f| %>
    <%= f.error_notification %>

    <div class="form-inputs">
      <%= f.input :name %>
    </div>
    <div>
      <%= f.input :photo %>
    </div>
    <%= f.input :photo_cache, as: :hidden %>

    <div class="form-actions">
      <%= f.button :submit %>
    </div>
  <% end %>
</div>
